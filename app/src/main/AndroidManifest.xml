<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="bredesh.medico">

    <uses-feature
        android:name="android.hardware.camera"
        android:required="true" />




    <application
        android:fullBackupContent="true"
        android:allowBackup="true"
        android:icon="@mipmap/ic_medico"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
                <action android:name="android.intent.action.VIEW" />
            </intent-filter>
        </activity>
        <activity android:name=".login.MainLoginActivity" />
        <activity android:name=".camera.VideoData" />

        <service
            android:name=".push_notifications.NotificationService"
            android:icon="@mipmap/ic_medico"
            android:process=":medico_process"
            android:label="Medico"
            />

    </application>



</manifest>